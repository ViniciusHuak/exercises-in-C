#include <stdio.h>

int maiorNumero(int a, int b);

int main() {
    int num1, num2, maior;

        printf("Digite o primeiro número inteiro: ");
    scanf("%d", &num1);
    printf("Digite o segundo número inteiro: ");
    scanf("%d", &num2);

    
    maior = maiorNumero(num1, num2);

    
    printf("O maior número entre %d e %d é: %d\n", num1, num2, maior);

    return 0;
}

int maiorNumero(int a, int b) {
    if (a > b) {
        return a;
    } else {
        return b;
    }
}
